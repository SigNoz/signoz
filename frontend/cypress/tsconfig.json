{
	"extends": "../tsconfig.json",
	"target": "es5",
	"lib": ["es5", "dom"],
	"compilerOptions": {
		"noEmit": true,
		// be explicit about types included
		// to avoid clashing with Jest types
		"types": ["cypress", "@testing-library/cypress"],
		"isolatedModules": false
	},
	"include": ["../node_modules/cypress", "./**/*.ts"]
}
