			const APP_ID = '<%= htmlWebpackPlugin.options.INTERCOM_APP_ID %>';

			(function () {
				var w = window;
				var ic = w.Intercom;
				if (typeof ic === 'function') {
					ic('reattach_activator');
					ic('update', w.intercomSettings);
				} else {
					var d = document;
					var i = function () {
						i.c(arguments);
					};
					i.q = [];
					i.c = function (args) {
						i.q.push(args);
					};
					w.Intercom = i;
					var l = function () {
						var s = d.createElement('script');
						s.type = 'text/javascript';
						s.async = true;
