{
  "id": "parse-default-postgres-access-log",
  "name": "Parse default postgres access log",
  "alias": "parse-default-postgres-access-log",
  "description": "Parse standard postgres access log",
  "enabled": true,
  "filter": {
    "op": "AND",
    "items": [
      {
        "key": {
          "type": "tag",
          "key": "source",
          "data_type": "string"
        },
        "op": "=",
        "value": "postgres"
      }
    ]
  },
  "config": [
    {
      "type": "grok_parser",
      "id": "parse-body-grok",
      "enabled": true,
      "orderId": 1,
      "name": "Parse Body",
      "parse_to": "attributes",
      "pattern": "%{GREEDYDATA}",
      "parse_from": "body"
    }
  ]
}